<?xml version="1.0" encoding="UTF-8"?>
<streambase-configuration xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.streambase.com/schemas/sbconf/">
   <java-vm>
      <jar file="${TIBCO_HOME}/ems/7.0/lib/tibjms.jar"/>
	<!-- the following two libraries are also required for EMS 7 -->
      <jar file="${TIBCO_HOME}/ems/7.0/lib/jms.jar"/>
      <jar file="${TIBCO_HOME}/ems/7.0/lib/tibcrypt.jar"/>
    <!-- Note: Using StreamBase CEP 7.3 with EMS 8 requires that JDK7 be used as an External JDK. 
         See http://docs.streambase.com/latest/topic/com.streambase.sb.ide.help/data/html/install/externalJDK.html
    -->
   </java-vm> 
   <operator-parameters>
       <operator-parameter name="JMS_PROVIDER" value="tibems"/>
   </operator-parameters>
</streambase-configuration>